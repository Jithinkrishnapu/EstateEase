{
  "expo": {
    "name": "EstateEase",
    "slug": "estateease",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "privacy": "public",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jithinkrishna1998.estateease",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jithinkrishna1998.estateease"
    },
    "extra": {
      "eas": {
        "projectId": "7611578b-ef08-4749-bc24-b83d31e26969"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/7611578b-ef08-4749-bc24-b83d31e26969"
    }
  }
}
